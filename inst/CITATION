citHeader("To cite cellcount in publications use:")

title <- "{cellcount}: automated enumeration of cells from fluorescent microscopy images"
year <- sub("-.*","",meta$Date)
url <- "https://github.com/HABF-SDI-NCCOS-NOAA/cellcount"
note <- sprintf("R package version %s", meta$Version)

bibentry(
  bibtype = "Manual",
  title = title,
  author = c(person("Tyler","Harman"),
             person("Kaytee","Pokrzywinski")),
  year = year,
  note = note,
  url = url,
  textVersion = paste0(
  "Tyler Harman, Kaytee Pokrzywinski (",year,"). ", title, ". ", note, ". ",
  url
  )
)
